# printing the contents of a char[] is possible via an intermediate variable

# Output:
# hi!
#

# Exit:
# 0

# Program:

begin
  int f(char[] u) is
    int i = 0;
    while (i < len u) do
      print u[i] ;
      i = i + 1
    done ;
    return 0
  end
  char[] s = ['h','i','!'];
  int z = call f(s)
end
